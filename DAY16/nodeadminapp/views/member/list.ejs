

        <!--우측 상단 헤더 영역 -->
        <div class="row wrapper border-bottom white-bg page-heading">
          <div class="col-lg-10">
            <h2>회원 목록 조회</h2>
            <ol class="breadcrumb">
              <li>
                <a href="/">Home -></a>
              </li>
              <li>
                <a>회원 목록 -></a>
              </li>
              <li class="active">
                <strong>조회</strong>
              </li>
            </ol>
          </div>
          <div class="col-lg-2"></div>
        </div>

        <!--우측 콘텐츠 영역 -->
        <div class="wrapper wrapper-content">
          <form action="/member/list" method="post" id="searchForm">
            <div class="ibox-content m-b-sm border-bottom">
              <div class="row">
                <div class="col-md-4">
                  <div class="form-group">
                    <label class="control-label" for="OrderNo">가입유형</label>
                    <select
                      name="entryTypeCode"
                      id="entryTypeCode"
                      class="form-control"
                    >
                      <option value="0" <% if( searchOption.entryTypeCode === "9") { %> selected <%} %>>전체</option>
                      <option value="1" <% if( searchOption.entryTypeCode === "0") { %> selected <%} %>>관리자멤버</option>
                      <option value="2" <% if( searchOption.entryTypeCode === "1") { %> selected <%} %>>일반멤버</option>
                    </select>

                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label class="control-label" for="OrderName">이메일</label>
                    <input
                      type="email"
                      name="email"
                      value="<%=searchOption.email%>"
                      class="form-control"
                    />
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label class="control-label" for="OrderName">이름</label>
                    <input
                      type="text"
                      name="name"
                      value="<%=searchOption.name%>"
                      class="form-control"
                    />
                  </div>
                </div>
              </div>
              <div class="text-center">
                <input type="submit" class="btn btn-primary" value="조회" />
                <a href="/article/create" class="btn btn-info">신규</a>
              </div>
            </div>
          </form>

          <div class="row">
            <div class="col-lg-12">
              <div class="ibox float-e-margins">
                <div class="ibox-content">
                  <div class="table-responsive">
                    <table class="table table-hover">
                      <thead>
                        <tr>
                          <th width="10%">멤버id</th>
                          <th width="10%">이메일</th>
                          <th width="20%">이름</th>
                          <th width="20%">전화번호</th>
                          <th width="10%">가입유형</th>
                          <th width="10%">생일</th>
                          <th width="20%">등록날짜</th>
                        </tr>
                      </thead>
                      <tbody class="hoverTblBody">
                        <% for (var i=0; i < memberList.length; i++) { %>

                        <tr>
                          <% Object.keys(memberList[i]).forEach(function(key) { %>
                          <% if (key === 'edit_date' || key === 'reg_member_id' || key === 'edit_member_id' || key === 'member_password' || key === 'profile_img_path' || key === 'entry_type_code') {
                          %> <% return; %> <% } %>
                          <td>
                            <% if (key === 'title') { %>
                            <a
                              href="/member/update?idx=<%=memberList[i][key]%>"
                            >
                              <%= memberList[i][key] %>
                            </a>
                            <% } else if (key === 'member_id') { %>

                            <a
                              href="/member/modify/<%=memberList[i]['member_id']%>"
                            >
                              <%= memberList[i][key] %>
                            </a>

                            <% } else if (key === 'isDisplayCode') { %> <% if
                            (memberList[i][key] === 1) { %>
                            <p>게시함</p>
                            <% } else { %>
                            <p>게시안함</p>
                            <% }%> <% } else { %>
                            <p><%= memberList[i][key] %></p>
                            <% } %>
                          </td>
                          <% }); %>
                        </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

